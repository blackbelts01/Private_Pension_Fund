<odoo>
    <data>
        <report
            id="member_fund"
            model="res.partner"
            string="Fund"
            report_type="qweb-pdf"
            name="Private_Pension_Fund.report_fund_view"
            file="Private_Pension_Fund.report_fund_view"
        />
        <template id="report_fund_view">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <p><h4><strong>Member ID<span t-field="doc.member_id"/></strong></h4></p>
                            <p><h4><strong>Member Name<span t-field="doc.name"/></strong></h4></p>

                            <table class="table table-condensed">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Subscription</th>
                                        <th>Percentage</th>
                                        <th>Unit Price</th>
                                        <th>units</th>
                                        <th>Fund</th>
                                        <th>Sales Price</th>
                                        <th>Total Price</th>
                                        <th>ROI</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>