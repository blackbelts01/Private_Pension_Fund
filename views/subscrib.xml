<odoo>
    <data>
        <record model="ir.ui.view" id="subscription_form_view">
            <field name="model">subcribtion.bb</field>
            <field name="inherit_id" ref="account.invoice_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='date_invoice']" position="after">
                    <field name="currency"/>
                    <field name="Alloctaed"/>
                    <field name="o_s"/>
                </xpath>
                <xpath expr="//page[1]" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//page[1]" position="after">
                <page string="Sub_lines">
                    <field name="sub_lines">
                        <tree>
                            <field name="mem_id"/>
                            <field name="mem_name"/>
                            <field name="salary"/>
                            <field name="perc_salary"/>
                            <field name="own"/>
                            <field name="company"/>
                             <field name="booster"/>
                             <field name="side"/>
                            <field name="sub_total"/>

                        </tree>
                    </field>
                </page>
            </xpath>


            </field>
        </record>
    </data>
</odoo>